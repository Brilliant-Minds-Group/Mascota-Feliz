@page
@model MascotaFeliz.App.Frontend.Pages.ListaDuenosModel
@{
}
<link rel="stylesheet" href="~/css/Estilos.css">
<h1 class="propietarios">Listado de Propietarios</h1>
<table class="table">
    <thead>
            <tr>
                <th>Nombre</th>
                <th>Apellidos</th>
                <th>Dirección</th>
                <th>Teléfono</th>
                <th>Detalles</th>
            </tr>
        </thead>
    
    @foreach (var dueno in Model.listaDuenos)
    {
        
        <tr>
            
           
            <td>@dueno.Nombres</td>
            <td>@dueno.Apellidos</td>
            <td>@dueno.Direccion</td>
            <td>@dueno.Telefono</td>
            
            <td>
                <a class = "btn-listado" asp-page="./DetallesDuenos" asp-route-duenoId="@dueno.Id">
                Detalle Dueno
                </a>
            </td>
            
        </tr>
    }
</table>